function a:
read A
%
	A:=(hd A)
%
write A


function testfunction:
read A,B,C
%
  if nil then
        A := (hd C) ;
        B := (cons (hd A) B)
  else
    foreach A in C do
    	for (cons A B) do
			if (A and B and (A or B) ) then
				while A do
					nop
				od
			fi
		od
    od
  fi;
  A := (a A)
%
write A,B,C,D

