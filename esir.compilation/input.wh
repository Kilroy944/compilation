function testfunction:
read A,B,C
%
  if nil then
      if (cons (hd A) B) then
       nop
      fi
  else
    while (hd (cons A C)) and (tl B) do
    	A,B := B,toto;
    	if (A =? B and C =? D) or A then
    		nop
    	fi
    od
  fi
%
write A,B,C

function testExp : 
read A,B
%
if not ((A =? B and C =? D) or A) then
    		nop
fi
%
write B


